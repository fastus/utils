  },
  "files": [
    "source"
  ],
  "dependencies": {
    "abl-constants": "git+ssh://github.com/bucket-list/constants.git#0.1.2",
    "abl-lang": "git+ssh://github.com/bucket-list/lang.git#0.1.8",
    "debug": "2.2.0",
    "escape-string-regexp": "1.0.5",
    "lodash": "4.6.1",
    "qs": "6.1.0",
    "rrule": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.6.4",
    "babel-core": "6.6.4",
    "babel-plugin-espower": "2.1.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint-config-trejgun": "git://github.com/TrejGun/eslint-config-trejgun.git#0.0.2",
    "istanbul": "^1.0.0-alpha",
    "mocha": "2.4.5",
    "power-assert": "1.2.0"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "scripts": {
    "postinstall": "babel -d ./build ./source",
    "build": "rm -rf build && babel -d ./build ./source",
    "pull": "git checkout master && git pull origin master",
    "push": "git push origin master --tags && exit 0 && npm publish",
    "commit": "git commit -a -m ${1}",
    "release": "npm run pull && npm version ${1:-patch} && npm run push",
    "lint": "eslint source/*",
    "test": "NODE_ENV=${NODE_ENV:=test} node_modules/.bin/babel-node node_modules/.bin/_mocha",
    "coverage": "NODE_ENV=${NODE_ENV:=test} babel-node node_modules/istanbul/lib/cli cover --report text --report html node_modules/.bin/_mocha test/utils"
  }
}
